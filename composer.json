{
    "name": "lara-zeus/chaos",
    "homepage": "https://larazeus.com",
    "support": {
        "issues": "https://github.com/lara-zeus/chaos/issues",
        "source": "https://github.com/lara-zeus/chaos"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "php coder",
            "email": "info@larazeus.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.1",
        "lara-zeus/popover": "1.x-dev",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "nunomaduro/collision": "^7.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "laravel/pint": "^1.0",
        "larastan/larastan": "^2.2",
        "orchestra/testbench": "^8.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Chaos\\": "src"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Chaos\\ChaosServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ]
}
